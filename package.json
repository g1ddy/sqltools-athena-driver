{
  "name": "sqltools-athena-driver-v2",
  "displayName": "SQLTools Driver for AWS Athena Engine v2",
  "description": "SQLTools Driver for AWS Athena Engine v2",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.42.0"
  },
  "publisher": "brunoventura",
  "license": "MIT",
  "icon": "icons/extension_icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/kovihq/sqltools-athena-driver"
  },
  "bugs": {},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "keywords": [
    "athena",
    "sqltools-driver"
  ],
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters",
    "Other"
  ],
  "extensionDependencies": [
    "mtxr.sqltools"
  ],
  "activationEvents": [
    "*",
    "onLanguage:sql",
    "onCommand:sqltools.*"
  ],
  "main": "./out/extension.js",
  "dependencies": {
    "@sqltools/base-driver": "latest",
    "@sqltools/types": "latest",
    "aws-sdk": "^2.868.0",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.5.5",
    "@types/node": "^14.0.9",
    "@types/vscode": "^1.42.0",
    "typescript": "^3.7.3"
  }
}
